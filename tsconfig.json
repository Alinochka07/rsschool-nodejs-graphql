{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "outDir": "dist",
    "sourceMap": true,
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "target": "ESNext",
    "allowImportingTsExtensions": true,
    "noEmit": true,
  },
  "include": ["src/**/*.ts"]
}
